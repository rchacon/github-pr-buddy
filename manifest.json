{
  "manifest_version": 2,
  "name": "GitHub PR Buddy",
  "description": "Chrome extension for displaying GitHub Pull Requests.",
  "version": "0.0.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "src/popup.html"
  },
  "options_page": "src/options.html",
  "permissions": [
    "storage",
    "https://api.github.com/repos/*/*/pulls"
  ]
}
